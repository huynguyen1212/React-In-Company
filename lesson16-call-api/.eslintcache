] = useState(\"\");\n  const [txtName, setTxtName] = useState(\"\");\n  const [txtPrice, setTxtPrice] = useState(\"\");\n  const [chkbStatus, setChkbStatus] = useState(\"\");\n\n  let history = useHistory();\n\n  useEffect(() => {\n    var { match } = props;\n    if (match) {\n      var id = match.params.id;\n      callApi(`products/${id}`, 'GET', null).then(res => {\n        var data = res.data;\n        setId(data.id);\n        setTxtName(data.name);\n        setTxtPrice(data.price);\n        setChkbStatus(data.status);\n      })\n    }\n  }, [props]);\n\n  const onChange = (e) => {\n    var target = e.target;\n    var name = target.name;\n    var value = target.type === \"checkbox\" ? target.checked : target.value;\n    if (name == \"id\") {\n      setId(value);\n    }\n    else if (name == \"txtName\") {\n      setTxtName(value);\n    }\n    else if (name == \"txtPrice\") {\n      setTxtPrice(value);\n    }\n    else if (name == \"chkbStatus\") {\n      setChkbStatus(value)\n    }\n  };\n\n  const onSave = (e) => {\n    e.preventDefault();\n    if (id) {\n      callApi(`products/${id}`, \"PUT\",\n        {\n          name: txtName,\n          price: txtPrice,\n          status: chkbStatus\n        }\n      ).then(res => {\n        history.push(\"/product-list\");\n      })\n    }\n    else {\n      callApi(\"products\", \"POST\",\n        {\n          name: txtName,\n          price: txtPrice,\n          status: chkbStatus\n        }\n      ).then(res => {\n        history.push(\"/product-list\");\n      })\n    }\n  }\n\n  return (\n    <div className=\"col-xs-6 col-sm-6 col-md-6 col-lg-6\">\n      <form onSubmit={onSave}>\n        <div className=\"form-group\">\n          <label>Tên sản phẩm : </label>\n          <input type=\"text\" className=\"form-control\" name=\"txtName\" value={txtName} onChange={onChange} />\n        </div>\n        <div className=\"form-group\">\n          <label>Giá : </label>\n          <input type=\"number\" className=\"form-control\" name=\"txtPrice\" value={txtPrice} onChange={onChange} />\n        </div>\n        <label>Trạng thái :</label> <br />\n        <div className=\"checkbox\">\n          <label>\n            <input type=\"checkbox\" name=\"chkbStatus\" value={chkbStatus} onChange={onChange} checked={chkbStatus} />\n            Còn hàng\n          </label>\n        </div>\n        <Link to=\"/product-list\" className=\"btn btn-danger mr-10\">\n          Trở lại\n        </Link>\n        <button type=\"submit\" className=\"btn btn-primary\">Lưu lại</button>\n      </form>\n    </div>\n  );\n}\n\nexport default ProductActionPage;\n","/home/huynguyen/Desktop/Thực tập/React-In-Company/lesson16-call-api/src/reducers/index.js",[],"/home/huynguyen/Desktop/Thực tập/React-In-Company/lesson16-call-api/src/reducers/products.js",[],"/home/huynguyen/Desktop/Thực tập/React-In-Company/lesson16-call-api/src/utils/apiCaller.js",[],"/home/huynguyen/Desktop/Thực tập/React-In-Company/lesson16-call-api/src/constants/Config.js",[],"/home/huynguyen/Desktop/Thực tập/React-In-Company/lesson16-call-api/src/constants/ActionTypes.js",[],"/home/huynguyen/Desktop/Thực tập/React-In-Company/lesson16-call-api/src/actions/index.js",[],{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","replacedBy":"85"},{"ruleId":"86","severity":1,"message":"87","line":55,"column":7,"nodeType":"88","endLine":55,"endColumn":35},{"ruleId":"89","severity":1,"message":"90","line":1,"column":28,"nodeType":"91","messageId":"92","endLine":1,"endColumn":36},{"ruleId":"93","severity":1,"message":"94","line":18,"column":6,"nodeType":"95","endLine":18,"endColumn":13,"suggestions":"96"},{"ruleId":"97","severity":1,"message":"98","line":32,"column":14,"nodeType":"99","messageId":"100","endLine":32,"endColumn":16},{"ruleId":"97","severity":1,"message":"98","line":35,"column":19,"nodeType":"99","messageId":"100","endLine":35,"endColumn":21},{"ruleId":"97","severity":1,"message":"98","line":38,"column":19,"nodeType":"99","messageId":"100","endLine":38